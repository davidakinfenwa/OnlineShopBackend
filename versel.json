{
    "builds": [{
        "src": "storeapi/storeapi/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    } 
     
],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "storeapi/storeapi/wsgi.py"
        }
    ]
}